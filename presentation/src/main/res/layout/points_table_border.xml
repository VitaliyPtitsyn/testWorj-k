<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <View
        android:id="@+id/bootmLine"
        android:layout_width="0dp"
        android:layout_height="@dimen/table_line_thinkes"
        android:background="@color/table_seporator"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <View
        android:id="@+id/order_seporator"
        android:layout_width="@dimen/table_line_thinkes"
        android:layout_height="0dp"
        android:background="@color/table_seporator"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/gl_order_end"
        app:layout_constraintStart_toStartOf="@+id/gl_order_end"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/points_seporator"
        android:layout_width="@dimen/table_line_thinkes"
        android:layout_height="0dp"
        android:background="@color/table_seporator"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/gl_poinss_seporator"
        app:layout_constraintStart_toStartOf="@+id/gl_poinss_seporator"
        app:layout_constraintTop_toTopOf="parent" />
</merge>